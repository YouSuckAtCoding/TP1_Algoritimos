
# Não é necessaria nenhuma alteração, em python a comparação é feita com strings com os operadoes padrão.
class BubbleSorter:
    def __init__(self, arr):
        self.__a = arr        
        self.__nItems = len(arr)  

    def bubbleSort(self):                           
      for last in range(self.__nItems-1, 0, -1):
         for inner in range(last):                
            if self.__a[inner] > self.__a[inner+1]:  
                self.__a[inner], self.__a[inner + 1] = self.__a[inner + 1], self.__a[inner] ;   

    def get_sorted_array(self):
        return self.__a

sorter = BubbleSorter(["aaaa", "bbbb", "ababab", "kappa", "vsf"])
sorter.bubbleSort()
print(sorter.get_sorted_array())